    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-form": "^8.3.3",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
